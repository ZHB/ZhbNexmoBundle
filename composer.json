{
    "name": "zhb/nexmo-bundle",
    "description": "Symfony Nexmo SMS integration",
    "keywords": [
        "nexmo",
        "sms"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Vincent Huck",
            "email": "vincent.huck.pro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "nexmo/client": "^1.4",
        "symfony/serializer": "^3.4 || ^4.1"
    },
    "autoload": {
        "psr-4": {
            "Zhb\\NexmoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhb\\NexmoBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/browser-kit": "^3.4 || ^4.0",
        "symfony/property-access": "^3.4 || ^4.1"
    }
}
